{
  "realm": "ospo-events",
  "auth-server-url": "http://keycloak:8080/auth/",
  "ssl-required": "none",
  "resource": "ospo-events-app",
  "public-client": true,
  "confidential-port": 0,
  "verify-token-audience": false,
  "credentials": {
    "secret": "supersecret"
  },
  "use-resource-role-mappings": true,
  "enable-cors": true,
  "enable-registration": true,
  "enable-pkce": true,
  "cors-exposed-headers": ["Authorization"]
}